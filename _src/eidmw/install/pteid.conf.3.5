#PTEID-MW Configure File
#Make sure you add more lines to the end to avoid line removal.

[crl]
crl_cachedir=/usr/share/pteid/crl

[certificatecache]
cert_cachefile=/usr/share/pteid/crl/.cache.csc

[certificatevalidation]
cert_validation_ocsp=0
cert_validation_crl=0

[configuretool]
show_toolbar=1
show_picture=0
show_notification=1
automatic_cardreading=0
cardreader=-1
registrate_certificate=1

[general]
language=en
